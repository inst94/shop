@model shop.Models.Cars.ImagesViewModel

<div class="form-group row col-sm-4 offset-2">
    <div>@Model.ImageTitle</div>
    <img class="card-img-top" src="@Model.Image" asp-append-version="true" />
    <input asp-action="removeImage" asp-controller="car" asp-route-imageId="@Model.Id" type="submit" class="btn btn-danger" value="Remove Image" />
</div>
